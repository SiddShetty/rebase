{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "rules": {
        "no-console": false,
        "no-submodule-imports": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "member-access": false,
        "no-unused-expression": [true, "allow-new"]
    }
}